<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0/EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>
	<!-- 
	VO / DTO Aliases
	<typeAliases>
		Alias example
		<typeAlias type="pathOfClass" alias="example"/>
	</typeAliases>
	-->
	<typeAliases>
		<typeAlias type="com.misoot.lar.admin.model.vo.Admin" alias="admin"/>
		<typeAlias type="com.misoot.lar.home.model.vo.Home" alias="home"/>
		<typeAlias type="com.misoot.lar.user.model.vo.User" alias="user"/>
		<typeAlias type="com.misoot.lar.commu.model.vo.Commu" alias="commu"/>
		<typeAlias type="com.misoot.lar.commu.model.vo.Attachment" alias="attachment"/>
