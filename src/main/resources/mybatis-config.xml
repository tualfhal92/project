		<typeAlias alias="BoardLectureAttachment" type="com.misoot.lar.lecture.model.vo.BoardLectureAttachment"/>
		<typeAlias alias="LectureTotalScore" type="com.misoot.lar.lecture.model.vo.LectureTotalScore"/>
	</typeAliases>
	
	<!--
	TypeHandlers
	<typeHandlers>
		TypeHandlers example	
		<package name="com.kh.spring.common.typehandler"/>
	</typeHandlers>
	-->
</configuration>
