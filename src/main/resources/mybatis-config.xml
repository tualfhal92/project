		<typeAlias type="com.misoot.lar.common.sockJS.model.vo.Inquire" alias="inquire"/>
		<typeAlias type="com.misoot.lar.user.model.vo.Purchase" alias="purchase"/>
		
		<!-- Lecture Vo -->
		<typeAlias alias="lecture" type="com.misoot.lar.lecture.model.vo.Lecture"/>		
		<typeAlias alias="lectureBoard" type="com.misoot.lar.lecture.model.vo.LectureBoard"/>		
		<typeAlias alias="LectureQ" type="com.misoot.lar.lecture.model.vo.LectureQ"/>		
		<typeAlias alias="LectureA" type="com.misoot.lar.lecture.model.vo.LectureA"/>		
